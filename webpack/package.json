{
  "name": "webpack",
  "version": "0.0.0",
  "description": "A React/Flux app generate by RF, powered with Babel",
  "main": "build/bundle.js",
  "dependencies": {
    "flux": "^2.0.3",
    "object-assign": "^2.0.0",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel-jest": "*",
    "babel-loader": "*",
    "bootstrap": "*",
    "bootstrap-webpack": "*",
    "compass-mixins": "^0.12.7",
    "css-loader": "*",
    "file-loader": "*",
    "imports-loader": "*",
    "jest-cli": "*",
    "jquery": "*",
    "node-sass": "*",
    "react-hammerjs": "^0.3.0",
    "react-hot-loader": "*",
    "react-simpletabs": "^0.7.0",
    "react-tools": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "underscore": "^1.8.3",
    "url-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js --colors --content-base-target 'http://lininliao.dev'",
    "dev-m": "webpack-dev-server --config webpack.dev.config.js --colors --content-base-target 'http://lininliao.dev' --host 192.168.1.115 --port 8080",
    "test": "jest"
  },
  "author": "",
  "license": "",
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globals": {
      "SCRIPTS": "<rootDir>/src/scripts"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
